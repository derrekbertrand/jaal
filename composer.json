{
    "name": "dialinno/jaal",
    "description": "A JSON:API Abstraction for Laravel.",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "ext-json": "*",
        "laravel/framework": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "orchestra/testbench": "^3.5",
        "orchestra/database": "^3.5",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "DialInno\\Jaal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DialInno\\Jaal\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DialInno\\Jaal\\JaalProvider"
            ]
        }
    }
}
